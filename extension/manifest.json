{
  "key": "",
  "name": "Cloud Debugger",
  "description": "Chrome DevTools over WebRTC",
  "version": "0.2",
  "manifest_version": 2,
  "permissions": [
    "debugger",
    "identity",
    "tabs",
    "https://*.google.com/",
    "https://www.googleapis.com/"
  ],
  "background": {
    "scripts": [
      "keys.js",
      "js/debugger_proxy.js",
      "js/device.js",
      "js/version_info.js",
      "js/user.js",
      "js/xhr.js",
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "images/debuggerPlay.png",
    "default_title": "Connect Cloud Debug",
    "default_popup": "menu.html"
  },
  "oauth2": {
    "client_id": "{CLIENT_ID}",
    "scopes": [
      "https://www.googleapis.com/auth/clouddevices"
    ]
  }
}
